import java.util.*;
public class Main {
    public static void main(String[] args) {
        int n = 5;
        for (int i = 0; i < n; i++) { 
            int num = 5;
            for (int j = 0; j <= i; j++) { 
                System.out.print(num + " ");
                num--;
            }
            for (int k = i + 1; k < n; k++) { 
                System.out.print(num + " ");
            }
            System.out.println();
        }
    }
}
